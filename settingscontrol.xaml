                                            Margin="0,2,0,0"
                                            HorizontalAlignment="Center"
                                            Orientation="Horizontal">
                                            <TextBox
                                                Name="MotionPitchOffset"
                                                Width="80"
                                                Margin="70,0,70,0"
                                                GotFocus="MotionPitchOffset_GotFocus"
                                                LostFocus="MotionPitchOffset_LostFocus"
                                                Style="{StaticResource EngineTBstyle}"
                                                Text="{Binding D.MotionPitchOffset}" />
                                            <TextBox
                                                Name="MotionPitchMult"
                                                Width="80"
                                                Margin="0,0,70,0"
                                                GotFocus="MotionPitchMult_GotFocus"
                                                LostFocus="MotionPitchMult_LostFocus"
