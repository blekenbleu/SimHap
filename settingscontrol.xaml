<UserControl
    x:Class="sierses.Sim.SettingsControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:sierses.Sim"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:styles="clr-namespace:SimHub.Plugins.Styles;assembly=SimHub.Plugins"
    d:DataContext="{d:DesignInstance}"
    mc:Ignorable="d">

    <UserControl.Resources>

        <!--  slider style for value label end vertical  -->
        <!--  corner radius textboxes
        <Style TargetType="{x:Type TextBox}">
            <Style.Resources>
                <Style TargetType="{x:Type Border}">
                    <Setter Property="CornerRadius" Value="6" />
                    <Setter Property="Height" Value="32" />
                    <Setter Property="BorderThickness" Value="0" />
                    <Setter Property="BorderBrush" Value="Transparent" />
                </Style>
            </Style.Resources>
        </Style>-->

        <!--  corner radius expander end  -->


        <ResourceDictionary>
<!-- needed for Designer debugging -->
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/sierses.Sim;component/Themes/SteelTheme.xaml">
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
<!-- gets overridden by ChangeTheme() -->  


            <Style x:Key="IconRectangle" TargetType="{x:Type Rectangle}">
                <Setter Property="Opacity" Value="0.9" />
                <Setter Property="Fill" Value="White" />
                <Setter Property="Height" Value="35" />
                <Setter Property="Width" Value="35" />
            </Style>
            <Style x:Key="StatusCircle" TargetType="{x:Type Rectangle}">
                <Setter Property="Opacity" Value="0.9" />
                <Setter Property="Fill" Value="White" />
                <Setter Property="Height" Value="10" />
                <Setter Property="Width" Value="10" />
            </Style>
            <Style x:Key="TabVis" TargetType="{x:Type StackPanel}">
                <Style.Triggers>
                    <Trigger Property="IsMouseOver" Value="true">
                        <Setter Property="Opacity" Value="0.6" />
                    </Trigger>
                </Style.Triggers>
            </Style>



            <!--  button style for eq repeat buttons  -->
            <Style x:Key="EQbutton" TargetType="{x:Type RepeatButton}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type RepeatButton}">
                            <Border
                                Background="{TemplateBinding Background}"
                                BorderThickness="0"
                                CornerRadius="5">
                                <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Opacity" Value="0.5" />
                                </Trigger>
                                <Trigger Property="IsMouseOver" Value="False">
                                    <Setter Property="Opacity" Value="1" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>


            <!--  button style for other buttons curved and opacity  -->
            <Style x:Key="dMASSbutton" TargetType="{x:Type Button}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border
                                Background="{TemplateBinding Background}"
                                BorderThickness="0"
                                CornerRadius="5">
                                <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Opacity" Value="0.5" />
                                </Trigger>
                                <Trigger Property="IsMouseOver" Value="False">
                                    <Setter Property="Opacity" Value="1" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>




            <!--  tab styling start
            <Style TargetType="{x:Type TabItem}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type TabItem}">
                            <Border>
                                <Grid>
                                    <Grid>
                                        <Border
                                            x:Name="border"
                                            Background="{DynamicResource Dark}"
                                            CornerRadius="12,12,0,0" />
                                    </Grid>
                                    <ContentPresenter
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                        ContentSource="Header" />
                                </Grid>
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter TargetName="border" Property="Background" Value="{DynamicResource Light}" />
                                </Trigger>
                                <Trigger Property="IsSelected" Value="True">
                                    <Setter TargetName="border" Property="Background" Value="{DynamicResource Mid}" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            tab styling end-->








            <!--  combobox styling start  -->
            <ControlTemplate x:Key="ComboBoxToggleButton" TargetType="{x:Type ToggleButton}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition Width="20" />
                    </Grid.ColumnDefinitions>
                    <Border
                        x:Name="Border"
                        Grid.ColumnSpan="2"
                        Background="{DynamicResource Mid}"
                        BorderBrush="{DynamicResource Mid}"
                        BorderThickness="0"
                        CornerRadius="6" />
                    <Border
                        Grid.Column="0"
                        Margin="1"
                        Background="{DynamicResource Dark}"
                        BorderBrush="{DynamicResource Mid}"
                        BorderThickness="0,0,1,0"
                        CornerRadius="6,0,0,6" />
                    <Path
                        x:Name="Arrow"
                        Grid.Column="1"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Data="M0,0 L0,2 L4,6 L8,2 L8,0 L4,4 z"
                        Fill="White" />
                </Grid>
                <!--<ControlTemplate.Triggers>
            <Trigger Property="ToggleButton.IsMouseOver" Value="true">
                <Setter TargetName="Border" Property="Background" Value="#808080" />
            </Trigger>
            <Trigger Property="ToggleButton.IsChecked" Value="true">
                <Setter TargetName="Border" Property="Background" Value="#E0E0E0" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Border" Property="Background" Value="#EEEEEE" />
                <Setter TargetName="Border" Property="BorderBrush" Value="#AAAAAA" />
                <Setter Property="Foreground" Value="#888888"/>
                <Setter TargetName="Arrow" Property="Fill" Value="#888888" />
            </Trigger>
        </ControlTemplate.Triggers>-->
            </ControlTemplate>

            <ControlTemplate x:Key="ComboBoxTextBox" TargetType="{x:Type TextBox}">
                <Border
                    x:Name="PART_ContentHost"
                    Background="{TemplateBinding Background}"
                    Focusable="False" />
            </ControlTemplate>

            <Style x:Key="{x:Type ComboBox}" TargetType="{x:Type ComboBox}">
                <Setter Property="SnapsToDevicePixels" Value="true" />
                <Setter Property="OverridesDefaultStyle" Value="true" />
                <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto" />
                <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto" />
                <Setter Property="ScrollViewer.CanContentScroll" Value="true" />
                <Setter Property="MinWidth" Value="80" />
                <Setter Property="MinHeight" Value="20" />
                <Setter Property="Foreground" Value="White" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type ComboBox}">
                            <Grid>
                                <ToggleButton
                                    Name="ToggleButton"
                                    Grid.Column="2"
                                    ClickMode="Press"
                                    Focusable="false"
                                    IsChecked="{Binding Path=IsDropDownOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                    Template="{StaticResource ComboBoxToggleButton}" />
                                <ContentPresenter
                                    Name="ContentSite"
                                    Margin="3,3,23,3"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Content="{TemplateBinding SelectionBoxItem}"
                                    ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
                                    ContentTemplateSelector="{TemplateBinding ItemTemplateSelector}"
                                    IsHitTestVisible="False" />
                                <TextBox
                                    x:Name="PART_EditableTextBox"
                                    Margin="3,3,23,3"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Background="#FF3F3F3F"
                                    Focusable="True"
                                    Foreground="Green"
                                    IsReadOnly="{TemplateBinding IsReadOnly}"
                                    Style="{x:Null}"
                                    Template="{StaticResource ComboBoxTextBox}"
                                    Visibility="Hidden" />
                                <Popup
                                    Name="Popup"
                                    AllowsTransparency="True"
                                    Focusable="False"
                                    IsOpen="{TemplateBinding IsDropDownOpen}"
                                    Placement="Bottom"
                                    PopupAnimation="Slide">

                                    <Grid
                                        Name="DropDown"
                                        MinWidth="{TemplateBinding ActualWidth}"
                                        MaxHeight="{TemplateBinding MaxDropDownHeight}"
                                        SnapsToDevicePixels="True">
                                        <Border
                                            x:Name="DropDownBorder"
                                            Background="{DynamicResource Dark}"
                                            BorderBrush="{DynamicResource Mid}"
                                            BorderThickness="1" />
                                        <ScrollViewer Margin="4,6,4,6" SnapsToDevicePixels="True">
                                            <StackPanel IsItemsHost="True" KeyboardNavigation.DirectionalNavigation="Contained" />
                                        </ScrollViewer>
                                    </Grid>
                                </Popup>
                            </Grid>
                            <ControlTemplate.Triggers>
                                <Trigger Property="HasItems" Value="false">
                                    <Setter TargetName="DropDownBorder" Property="MinHeight" Value="95" />
                                </Trigger>
                                <Trigger Property="IsEnabled" Value="false">
                                    <Setter Property="Foreground" Value="#888888" />
                                </Trigger>
                                <Trigger Property="IsGrouping" Value="true">
                                    <Setter Property="ScrollViewer.CanContentScroll" Value="false" />
                                </Trigger>
                                <Trigger SourceName="Popup" Property="Popup.AllowsTransparency" Value="true">
                                    <Setter TargetName="DropDownBorder" Property="CornerRadius" Value="0" />
                                    <Setter TargetName="DropDownBorder" Property="Margin" Value="0,2,0,0" />
                                </Trigger>
                                <Trigger Property="IsEditable" Value="true">
                                    <Setter Property="IsTabStop" Value="false" />
                                    <Setter TargetName="PART_EditableTextBox" Property="Visibility" Value="Visible" />
                                    <Setter TargetName="ContentSite" Property="Visibility" Value="Hidden" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Style.Triggers />
            </Style>

            <!--  SimpleStyles: ComboBoxItem  -->
            <Style x:Key="{x:Type ComboBoxItem}" TargetType="{x:Type ComboBoxItem}">
                <Setter Property="SnapsToDevicePixels" Value="true" />
                <Setter Property="Foreground" Value="White" />
                <Setter Property="OverridesDefaultStyle" Value="true" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type ComboBoxItem}">
                            <Border
                                Name="Border"
                                Padding="2"
                                SnapsToDevicePixels="true">
                                <ContentPresenter />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsHighlighted" Value="true">
                                    <Setter TargetName="Border" Property="Background" Value="#FF4F4F4F" />
                                </Trigger>
                                <Trigger Property="IsEnabled" Value="false">
                                    <Setter Property="Foreground" Value="#888888" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <!--  combobox styling end  -->




            <!--  Button corner radius and styling  -->

            <Style TargetType="{x:Type Button}">
                <Style.Resources>
                    <Style TargetType="{x:Type Border}">
                        <Setter Property="CornerRadius" Value="6" />
                        <Setter Property="BorderThickness" Value="0" />
                    </Style>
                </Style.Resources>
            </Style>





            <!--  slider style !!!NEW!!! horizontal and vertical  -->

            <Style x:Key="Style_SliderLeftButton" TargetType="{x:Type RepeatButton}">
                <Setter Property="SnapsToDevicePixels" Value="True" />
                <Setter Property="OverridesDefaultStyle" Value="True" />
                <Setter Property="IsTabStop" Value="False" />
                <Setter Property="Focusable" Value="False" />
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type RepeatButton}">
                            <Border
                                Height="4"
                                Margin="8,0,-8,0"
                                Background="{TemplateBinding Background}"
                                CornerRadius="2" />
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style x:Key="Style_SliderRightButton" TargetType="{x:Type RepeatButton}">
                <Setter Property="SnapsToDevicePixels" Value="True" />
                <Setter Property="OverridesDefaultStyle" Value="True" />
                <Setter Property="IsTabStop" Value="False" />
                <Setter Property="Focusable" Value="False" />
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type RepeatButton}">
                            <Border
                                Height="4"
                                Margin="0,0,8,0"
                                Background="{TemplateBinding Background}"
                                CornerRadius="2" />
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style x:Key="Style_SliderThumb" TargetType="{x:Type Thumb}">
                <Setter Property="SnapsToDevicePixels" Value="True" />
                <Setter Property="OverridesDefaultStyle" Value="True" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Thumb}">
                            <StackPanel Orientation="Vertical">
                                <Grid>
                                    <!--  Big point  -->
                                    <Path
                                        x:Name="ThumbdMASSnew"
                                        Margin="20,8,0,0"
                                        Data="M 1 1 L 1 1 L 1 1 Z"
                                        Fill="{DynamicResource Mid}"
                                        Stroke="{DynamicResource Mid}"
                                        StrokeThickness="30" />







                                </Grid>

                                <!--  text label  -->
                                <TextBlock
                                    x:Name="ThumbdMASSlabelnew"
                                    Margin="5,-24,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Background="Transparent"
                                    FontFamily="roboto"
                                    FontSize="14"
                                    Foreground="white"
                                    Text="{Binding Value, RelativeSource={RelativeSource AncestorType={x:Type Slider}, Mode=FindAncestor}}"
                                    TextAlignment="Center" />
                            </StackPanel>
                            <!--  mouse hover color change  -->
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter TargetName="ThumbdMASSnew" Property="Stroke" Value="{DynamicResource Dark}" />
                                    <Setter TargetName="ThumbdMASSlabelnew" Property="Opacity" Value="0.6" />
                                </Trigger>
                            </ControlTemplate.Triggers>

                        </ControlTemplate>

                    </Setter.Value>
                </Setter>
            </Style>

            <!--  Template when the orientation of the Slider is Horizontal.  -->
            <ControlTemplate x:Key="HorizontalSlider" TargetType="{x:Type Slider}">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" MinHeight="{TemplateBinding MinHeight}" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>

                    <TickBar
                        x:Name="TopTick"
                        Height="4"
                        Fill="white"
                        Placement="Top"
                        SnapsToDevicePixels="True"
                        Visibility="Collapsed" />

                    <Border
                        x:Name="TrackBackground"
                        Grid.Row="1"
                        Height="3"
                        Margin="8,-7,0,0"
                        Background="{DynamicResource Slider}"
                        BorderThickness="0"
                        CornerRadius="2" />

                    <Track x:Name="PART_Track" Grid.Row="1">

                        <Track.DecreaseRepeatButton>
                            <RepeatButton Command="Slider.DecreaseLarge" Style="{StaticResource Style_SliderLeftButton}" />
                        </Track.DecreaseRepeatButton>

                        <Track.Thumb>
                            <Thumb Style="{StaticResource Style_SliderThumb}" />
                        </Track.Thumb>

                        <Track.IncreaseRepeatButton>
                            <RepeatButton Command="Slider.IncreaseLarge" Style="{StaticResource Style_SliderRightButton}" />
                        </Track.IncreaseRepeatButton>
                    </Track>

                    <TickBar
                        x:Name="BottomTick"
                        Grid.Row="2"
                        Height="4"
                        Fill="{TemplateBinding Foreground}"
                        Placement="Bottom"
                        SnapsToDevicePixels="True"
                        Visibility="Collapsed" />
                </Grid>

                <ControlTemplate.Triggers>
                    <Trigger Property="TickPlacement" Value="TopLeft">
                        <Setter TargetName="TopTick" Property="Visibility" Value="Visible" />
                    </Trigger>
                    <Trigger Property="TickPlacement" Value="BottomRight">
                        <Setter TargetName="BottomTick" Property="Visibility" Value="Visible" />
                    </Trigger>
                    <Trigger Property="TickPlacement" Value="Both">
                        <Setter TargetName="TopTick" Property="Visibility" Value="Visible" />
                        <Setter TargetName="BottomTick" Property="Visibility" Value="Visible" />
                    </Trigger>
                </ControlTemplate.Triggers>
            </ControlTemplate>

            <!--  Template when the orientation of the Slider is Vertical.  -->
            <ControlTemplate x:Key="VerticalSlider" TargetType="{x:Type Slider}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" MinWidth="{TemplateBinding MinWidth}" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>

                    <TickBar
                        x:Name="TopTick"
                        Width="4"
                        Fill="white"
                        Placement="Left"
                        SnapsToDevicePixels="True"
                        Visibility="Collapsed" />

                    <!--  background line  -->
                    <Border
                        x:Name="TrackBackground"
                        Grid.Column="1"
                        Width="5"
                        Margin="6,0,0,0"
                        Background="{DynamicResource Slider}"
                        BorderThickness="1"
                        CornerRadius="2" />

                    <Track x:Name="PART_Track" Grid.Column="1">

                        <Track.DecreaseRepeatButton>
                            <RepeatButton Command="Slider.DecreaseLarge" Style="{StaticResource Style_SliderLeftButton}" />
                        </Track.DecreaseRepeatButton>

                        <Track.Thumb>
                            <Thumb Style="{StaticResource Style_SliderThumb}" />
                        </Track.Thumb>

                        <Track.IncreaseRepeatButton>
                            <RepeatButton Command="Slider.IncreaseLarge" Style="{StaticResource Style_SliderRightButton}" />
                        </Track.IncreaseRepeatButton>
                    </Track>

                    <TickBar
                        x:Name="BottomTick"
                        Grid.Column="2"
                        Width="4"
                        Fill="{TemplateBinding Foreground}"
                        Placement="Right"
                        SnapsToDevicePixels="True"
                        Visibility="Collapsed" />
                </Grid>
                <ControlTemplate.Triggers>
                    <Trigger Property="TickPlacement" Value="TopLeft">
                        <Setter TargetName="TopTick" Property="Visibility" Value="Visible" />
                    </Trigger>
                    <Trigger Property="TickPlacement" Value="BottomRight">
                        <Setter TargetName="BottomTick" Property="Visibility" Value="Visible" />
                    </Trigger>
                    <Trigger Property="TickPlacement" Value="Both">
                        <Setter TargetName="TopTick" Property="Visibility" Value="Visible" />
                        <Setter TargetName="BottomTick" Property="Visibility" Value="Visible" />
                    </Trigger>
                </ControlTemplate.Triggers>
            </ControlTemplate>

            <Style TargetType="{x:Type Slider}">
                <Setter Property="SnapsToDevicePixels" Value="True" />
                <Setter Property="OverridesDefaultStyle" Value="True" />
                <Style.Triggers>
                    <Trigger Property="Orientation" Value="Horizontal">
                        <!--<Setter Property="MinWidth"
                    Value="104" />
            <Setter Property="MinHeight"
                    Value="21" />-->
                        <Setter Property="Template" Value="{StaticResource HorizontalSlider}" />
                    </Trigger>
                    <Trigger Property="Orientation" Value="Vertical">
                        <!--<Setter Property="MinWidth"
                    Value="21" />
            <Setter Property="MinHeight"
                    Value="104" />-->
                        <Setter Property="Template" Value="{StaticResource VerticalSlider}" />
                    </Trigger>
                </Style.Triggers>
            </Style>

            <!--  corner radius expander start  -->
            <Style x:Key="ExpanderToggleButton" TargetType="{x:Type ToggleButton}">
                <Setter Property="UseLayoutRounding" Value="True" />
                <Setter Property="SnapsToDevicePixels" Value="True" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type ToggleButton}">
                            <Grid Width="24" Height="24">
                                <Border
                                    x:Name="border"
                                    Background="{DynamicResource Dark}"
                                    CornerRadius="12" />
                                <Path
                                    x:Name="arrow"
                                    Data="M4.5 9 l4 0 l3.5 4.6667 l3.5 -4.6667 l4 0 l-7.5 10 z"
                                    Fill="White" />
                            </Grid>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter TargetName="border" Property="Background" Value="DarkGray" />
                                    <Setter TargetName="arrow" Property="Fill" Value="LightGray" />
                                </Trigger>
                                <Trigger Property="IsChecked" Value="True">
                                    <Setter TargetName="border" Property="Background" Value="White" />
                                    <Setter TargetName="arrow" Property="Fill" Value="{DynamicResource Dark}" />
                                    <Setter TargetName="arrow" Property="Data" Value="M4.5 15 l4 0 l3.5 -4.6667 l3.5 4.6667 l4 0 l-7.5 -10 z" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style TargetType="{x:Type Expander}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Expander}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition x:Name="ContentRow" Height="0" />
                                </Grid.RowDefinitions>
                                <Border
                                    x:Name="Border"
                                    Grid.Row="0"
                                    Background="{DynamicResource Mid}"
                                    BorderThickness="0"
                                    CornerRadius="2,0,10,0">
                                    <!--  color arround arrow  -->
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="31" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <ToggleButton
                                            Grid.Column="0"
                                            IsChecked="{Binding IsExpanded, Mode=TwoWay, RelativeSource={RelativeSource Mode=TemplatedParent}}"
                                            OverridesDefaultStyle="True"
                                            Style="{StaticResource ExpanderToggleButton}" />
                                        <!--  This is the border that puts the styling around the Expander Header  -->
                                        <!--  Add more detail here for a fancier header.  -->
                                        <Border
                                            Grid.Column="1"
                                            Background="{DynamicResource Mid}"
                                            CornerRadius="0,0,10,0">
                                            <ContentPresenter
                                                Height="30"
                                                Margin="10,5,0,0"
                                                ContentSource="Header"
                                                RecognizesAccessKey="True"
                                                TextBlock.FontFamily="roboto"
                                                TextBlock.FontSize="20"
                                                TextBlock.Foreground="White" />
                                        </Border>

                                    </Grid>
                                </Border>
                                <Border
                                    x:Name="Content"
                                    Grid.Row="1"
                                    BorderThickness="0"
                                    CornerRadius="0,0,2,2">
                                    <ContentPresenter Margin="4" />
                                </Border>
                            </Grid>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsExpanded" Value="True">
                                    <Setter TargetName="ContentRow" Property="Height" Value="{Binding DesiredSize, ElementName=Content}" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </ResourceDictionary>
    </UserControl.Resources>

    <!--  end of resources  -->





    <!--  start  -->

    <Border
        BorderBrush="{DynamicResource Background}"
        BorderThickness="2"
        CornerRadius="5">
        <Grid>
            <Grid Background="{DynamicResource Background}">
                <Grid.ContextMenu>
                    <ContextMenu Height="600">
                        <!--dark themes-->
                        <MenuItem Click="GreenTheme" Header="Green" />
                        <MenuItem Click="BlueTheme" Header="Blue" />
                        <MenuItem Click="PurpleTheme" Header="Purple" />
                        <MenuItem Click="OrangeTheme" Header="Orange" />
                        <MenuItem Click="EmeraldTheme" Header="Emerald" />
                        <MenuItem Click="TealTheme" Header="Teal" />
                        <MenuItem Click="CobaltTheme" Header="Cobalt" />
                        <MenuItem Click="MagentaTheme" Header="Magenta" />
                        <MenuItem Click="CrimsonTheme" Header="Crimson" />
                        <MenuItem Click="AmberTheme" Header="Amber" />
                        <MenuItem Click="OliveTheme" Header="Olive" />
                        <MenuItem Click="SteelTheme" Header="Steel" />
                        <MenuItem Click="MauveTheme" Header="Mauve" />
                        <MenuItem Click="TaupeTheme" Header="Taupe" />

                        <Separator />

                        <!--light themes-->
                        <MenuItem Click="GreenLightTheme" Header="Green Light" />
                        <MenuItem Click="BlueLightTheme" Header="Blue Light" />
                        <MenuItem Click="PurpleLightTheme" Header="Purple Light" />
                        <MenuItem Click="OrangeLightTheme" Header="Orange Light" />
                        <MenuItem Click="EmeraldLightTheme" Header="Emerald Light" />
                        <MenuItem Click="TealLightTheme" Header="Teal Light" />
                        <MenuItem Click="CobaltLightTheme" Header="Cobalt Light" />
                        <MenuItem Click="MagentaLightTheme" Header="Magenta Light" />
                        <MenuItem Click="CrimsonLightTheme" Header="Crimson Light" />
                        <MenuItem Click="AmberLightTheme" Header="Amber Light" />
                        <MenuItem Click="OliveLightTheme" Header="Olive Light" />
                        <MenuItem Click="SteelLightTheme" Header="Steel Light" />
                        <MenuItem Click="MauveLightTheme" Header="Mauve Light" />
                        <MenuItem Click="TaupeLightTheme" Header="Taupe Light" />
                    </ContextMenu>
                </Grid.ContextMenu>

                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="30" />
                </Grid.RowDefinitions>

                <ScrollViewer VerticalScrollBarVisibility="Hidden">
                    <StackPanel
                        Margin="0,-2,0,0"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Top"
                        Orientation="vertical">
                        <!--  EXPANDER Engine RPM properties  -->
                        <Expander
                            Margin="5"
                            BorderThickness="2"
                            FontFamily="roboto"
                            FontSize="15"
                            IsExpanded="True">
                            <Expander.Header>
                                <StackPanel Margin="-4,-2,0,0" Orientation="Horizontal">
                                    <Image
                                        Width="31"
                                        Margin="-2,-3,10,0"
                                        Source="Resources\engine50x50.png" />
                                    <TextBlock
                                        Margin="-7,-5,0,0"
                                        VerticalAlignment="Center"
                                        FontFamily="roboto"
                                        FontSize="20"
                                        Foreground="White">
                                        Engine
                                    </TextBlock>
                                </StackPanel>
                            </Expander.Header>
                            <StackPanel>

                                <!--  CarName  -->
                                <StackPanel
                                    Margin="0,5,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Orientation="Horizontal">
                                    <TextBlock
                                        VerticalAlignment="Center"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Right"><Run Text="Name" /></TextBlock>
                                </StackPanel>
                                <StackPanel Margin="0,2,0,0">
                                    <TextBox
                                        x:Name="CarName"
                                        Width="400"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        Background="Transparent"
                                        BorderBrush="{DynamicResource Mid}"
                                        BorderThickness="0,0,0,2"
                                        CaretBrush="{DynamicResource Foreground}"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Foreground="{DynamicResource Foreground}"
                                        Text="{Binding S.CarName}" />
                                </StackPanel>

                                <!--  ID  -->
                                <StackPanel
                                    Margin="0,10,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Orientation="Horizontal">
                                    <TextBlock
                                        VerticalAlignment="Center"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Right"><Run Text="ID" /></TextBlock>
                                </StackPanel>
                                <StackPanel Margin="0,2,0,0">
                                    <TextBox
                                        x:Name="CarID"
                                        Width="400"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        Background="Transparent"
                                        BorderBrush="{DynamicResource Mid}"
                                        BorderThickness="0,0,0,2"
                                        CaretBrush="{DynamicResource Foreground}"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Foreground="{DynamicResource Foreground}"
                                        Text="{Binding S.Id}" />
                                </StackPanel>

                                <!--  Category  -->
                                <StackPanel
                                    Margin="0,10,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Orientation="Horizontal">
                                    <TextBlock
                                        VerticalAlignment="Center"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Right"><Run Text="Category" /></TextBlock>
                                </StackPanel>
                                <StackPanel Margin="0,2,0,0">
                                    <TextBox
                                        x:Name="CarClass"
                                        Width="400"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        Background="Transparent"
                                        BorderBrush="{DynamicResource Mid}"
                                        BorderThickness="0,0,0,2"
                                        CaretBrush="{DynamicResource Foreground}"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Foreground="{DynamicResource Foreground}"
                                        Text="{Binding S.Category}" />
                                </StackPanel>


                                <!--  Idle/Redline/MaxRPM headers  -->
                                <StackPanel
                                    Margin="0,10,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Orientation="Horizontal">
                                    <TextBlock
                                        MinWidth="80"
                                        Margin="0,0,80,0"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Center"><Run Text="Idle" /></TextBlock>
                                    <TextBlock
                                        MinWidth="80"
                                        Margin="0,0,80,0"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Center"><Run Text="Redline" /></TextBlock>
                                    <TextBlock
                                        MinWidth="80"
                                        Margin="0,0,0,0"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Center"><Run Text="Max" /></TextBlock>
                                </StackPanel>
                                <!--  Idle/Redline/MaxRPM textboxes  -->
                                <StackPanel
                                    Margin="0,2,0,0"
                                    HorizontalAlignment="Center"
                                    Orientation="Horizontal">
                                    <TextBox
                                        x:Name="IdleRPM"
                                        MinWidth="80"
                                        Margin="0,0,80,0"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        Background="Transparent"
                                        BorderBrush="{DynamicResource Mid}"
                                        BorderThickness="0,0,0,2"
                                        CaretBrush="{DynamicResource Foreground}"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Foreground="{DynamicResource Foreground}"
                                        Text="{Binding S.IdleRPM}" />
                                    <TextBox
                                        x:Name="Redline"
                                        MinWidth="80"
                                        Margin="0,0,80,0"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        Background="Transparent"
                                        BorderBrush="{DynamicResource Mid}"
                                        BorderThickness="0,0,0,2"
                                        CaretBrush="{DynamicResource Foreground}"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Foreground="{DynamicResource Foreground}"
                                        Text="{Binding S.Redline}" />
                                    <TextBox
                                        x:Name="MaxRPM"
                                        MinWidth="80"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        Background="Transparent"
                                        BorderBrush="{DynamicResource Mid}"
                                        BorderThickness="0,0,0,2"
                                        CaretBrush="{DynamicResource Foreground}"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Foreground="{DynamicResource Foreground}"
                                        Text="{Binding S.MaxRPM}" />
                                </StackPanel>

                                <!--  Engine config/cylinders/drivetrain headers  -->
                                <StackPanel
                                    Margin="0,10,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Orientation="Horizontal">
                                    <TextBlock
                                        MinWidth="80"
                                        Margin="0,0,80,0"
                                        VerticalAlignment="Center"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Center"><Run Text="Config" /></TextBlock>
                                    <TextBlock
                                        MinWidth="80"
                                        Margin="0,0,80,0"
                                        VerticalAlignment="Center"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Center"><Run Text="Cylinders" /></TextBlock>
                                    <TextBlock
                                        MinWidth="80"
                                        VerticalAlignment="Center"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Center"><Run Text="Drivetrain" /></TextBlock>
                                </StackPanel>

                                <!--  Engine config/cylinders/drivetrain comboboxes  -->
                                <StackPanel
                                    Margin="0,2,0,0"
                                    HorizontalAlignment="Center"
                                    Orientation="Horizontal">
                                    <!--  engine config  -->
                                    <ComboBox
                                        Name="EngineConfig"
                                        Width="80"
                                        Height="25.2"
                                        Margin="0,5,80,0"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Text="{Binding S.EngineConfiguration}">
                                        <ComboBoxItem Content="V" />
                                        <ComboBoxItem Content="I" />
                                        <ComboBoxItem Content="F" />
                                        <ComboBoxItem Content="E" />
                                        <ComboBoxItem Content="W" />
                                        <ComboBoxItem Content="R" />
                                    </ComboBox>
                                    <!--  Cylinders  -->
                                    <ComboBox
                                        Name="EngineCylinders"
                                        Width="80"
                                        Height="25.2"
                                        Margin="0,5,80,0"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Text="{Binding S.EngineCylinders}">
                                        <ComboBoxItem Content="1" />
                                        <ComboBoxItem Content="2" />
                                        <ComboBoxItem Content="3" />
                                        <ComboBoxItem Content="4" />
                                        <ComboBoxItem Content="5" />
                                        <ComboBoxItem Content="6" />
                                        <ComboBoxItem Content="8" />
                                        <ComboBoxItem Content="10" />
                                        <ComboBoxItem Content="12" />
                                        <ComboBoxItem Content="16" />
                                    </ComboBox>
                                    <ComboBox
                                        Name="Drivetrain"
                                        Width="80"
                                        Height="25.2"
                                        Margin="0,5,0,0"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Text="{Binding S.PoweredWheels}">
                                        <ComboBoxItem Content="F" />
                                        <ComboBoxItem Content="R" />
                                        <ComboBoxItem Content="A" />
                                    </ComboBox>
                                </StackPanel>

                                <!--  Engine location/hp/nm headers  -->
                                <StackPanel
                                    Margin="0,10,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Orientation="Horizontal">

                                    <TextBlock
                                        MinWidth="80"
                                        Margin="0,0,80,0"
                                        VerticalAlignment="Center"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Center"><Run Text="Location" /></TextBlock>
                                    <TextBlock
                                        MinWidth="80"
                                        Margin="0,0,80,0"
                                        VerticalAlignment="Center"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Center"><Run Text="HP" /></TextBlock>
                                    <TextBlock
                                        MinWidth="80"
                                        VerticalAlignment="Center"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Center"><Run Text="NM" /></TextBlock>
                                </StackPanel>
                                <!--  Engine location/hp/nm comboboxes and textboxes  -->
                                <StackPanel
                                    Margin="0,2,0,0"
                                    HorizontalAlignment="Center"
                                    Orientation="Horizontal">
                                    <ComboBox
                                        Name="Englocation"
                                        Width="80"
                                        Height="25.2"
                                        Margin="0,5,80,0"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Text="{Binding S.EngineLocation}">
                                        <ComboBoxItem Content="F" />
                                        <ComboBoxItem Content="FM" />
                                        <ComboBoxItem Content="M" />
                                        <ComboBoxItem Content="R" />
                                        <ComboBoxItem Content="RM" />
                                    </ComboBox>
                                    <TextBox
                                        x:Name="MaxPowerHP"
                                        MinWidth="80"
                                        Margin="0,0,80,0"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        Background="Transparent"
                                        BorderBrush="{DynamicResource Mid}"
                                        BorderThickness="0,0,0,2"
                                        CaretBrush="{DynamicResource Foreground}"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Foreground="{DynamicResource Foreground}"
                                        Text="{Binding S.MaxPower}" />
                                    <TextBox
                                        x:Name="MaxTorqueNM"
                                        MinWidth="80"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        Background="Transparent"
                                        BorderBrush="{DynamicResource Mid}"
                                        BorderThickness="0,0,0,2"
                                        CaretBrush="{DynamicResource Foreground}"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Foreground="{DynamicResource Foreground}"
                                        Text="{Binding S.MaxTorque}" />
                                </StackPanel>


                                <!--  Multi/EHP/CC Headers  -->
                                <StackPanel
                                    Margin="0,10,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Orientation="Horizontal">
                                    <TextBlock
                                        MinWidth="80"
                                        Margin="-7,0,70,0"
                                        VerticalAlignment="Center"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Center"><Run Text="PerGame Gain" /></TextBlock>
                                    <TextBlock
                                        MinWidth="80"
                                        Margin="0,0,62,0"
                                        VerticalAlignment="Center"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Center"><Run Text="EHP" /></TextBlock>
                                    <TextBlock
                                        MinWidth="80"
                                        Margin="18,0,0,0"
                                        VerticalAlignment="Center"
                                        Foreground="{DynamicResource Foreground}"
                                        TextAlignment="Center"><Run Text="CC" /></TextBlock>
                                </StackPanel>

                                <!--  Multi/EHP/CC Textboxes  -->
                                <StackPanel
                                    Height="35"
                                    Margin="-8,2,0,0"
                                    HorizontalAlignment="Center"
                                    Orientation="Horizontal">
                                    <!--  slider for engine per game multi  -->
                                    <Slider
                                        x:Name="EngineMulti"
                                        Width="88"
                                        Margin="0,0,80,0"
                                        VerticalAlignment="Bottom"
                                        VerticalContentAlignment="Bottom"
                                        IsSnapToTickEnabled="True"
                                        Maximum="1.5"
                                        Minimum="0.5"
                                        TickFrequency="0.1"
                                        ValueChanged="EngineMulti_ValueChanged"
                                        Value="{Binding D.EngineMult}" />
                                    <TextBox
                                        x:Name="EMaxPowerHP"
                                        MinWidth="80"
                                        Margin="0,0,80,0"
                                        VerticalAlignment="top"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        Background="Transparent"
                                        BorderBrush="{DynamicResource Mid}"
                                        BorderThickness="0,0,0,2"
                                        CaretBrush="{DynamicResource Foreground}"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Foreground="{DynamicResource Foreground}"
                                        Text="{Binding S.ElectricMaxPower}" />
                                    <!--  CC  -->
                                    <TextBox
                                        x:Name="Displacement"
                                        MinWidth="80"
                                        VerticalAlignment="top"
                                        HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center"
                                        Background="Transparent"
                                        BorderBrush="{DynamicResource Mid}"
                                        BorderThickness="0,0,0,2"
                                        CaretBrush="{DynamicResource Foreground}"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        Foreground="{DynamicResource Foreground}"
                                        Text="{Binding S.Displacement}" />
                                </StackPanel>


                                <!--  Refresh & Lock  -->
                                <StackPanel
                                    Margin="0,15,0,4"
                                    HorizontalAlignment="Right"
                                    Orientation="Horizontal">

                                    <!--  Lock/unlock button  -->
                                    <StackPanel HorizontalAlignment="Right">
                                        <Button
                                            Width="150"
                                            Height="35"
                                            VerticalContentAlignment="Center"
                                            Background="{DynamicResource Mid}"
                                            BorderThickness="0"
                                            Click="Lock_Click"
                                            ContentControl.Content="{Binding D.LockedText}"
                                            FontFamily="roboto"
                                            FontSize="16"
                                            FontWeight="SemiBold"
                                            Foreground="White"
                                            Style="{StaticResource dMASSbutton}" />




                                    </StackPanel>
                                    <!--  Refresh button  -->
                                    <Button
                                        Width="150"
                                        Height="35"
                                        Margin="10,0,0,5"
                                        VerticalContentAlignment="Center"
                                        Background="{DynamicResource Mid}"
                                        BorderThickness="0"
                                        Click="Refresh_Click"
                                        FontFamily="roboto"
                                        FontSize="16"
                                        FontWeight="SemiBold"
                                        Foreground="White"
                                        FrameworkElement.Name="Refresh"
                                        Style="{StaticResource dMASSbutton}">
                                        <StackPanel Orientation="Horizontal">
                                            <Image Width="31" Source="Resources\refresh50x50.png" />
                                            <TextBlock
                                                Margin="10,0,11,0"
                                                VerticalAlignment="Center"
                                                FontFamily="roboto"
                                                FontSize="16"
                                                Text="Refresh" />
                                        </StackPanel>

                                    </Button>
                                </StackPanel>
                            </StackPanel>
                        </Expander>


                        <!--  EQ SECTION  -->
                        <!--  EQ container  -->
                        <StackPanel>
                            <Expander
                                x:Name="EQ"
                                Margin="5,-1,5,3"
                                Expander.Expanded="EQ_Expanded"
                                FontFamily="roboto"
                                FontSize="16">
                                <Expander.Header>
                                    <StackPanel Margin="-4,-2,0,0" Orientation="Horizontal">
                                        <Image
                                            Width="29"
                                            Margin="0,-3,10,0"
                                            Source="Resources\eq.png" />
                                        <TextBlock
                                            Margin="-7,-5,0,0"
                                            VerticalAlignment="Center"
                                            FontFamily="roboto"
                                            FontSize="20"
                                            Foreground="White">
                                            Engine - EQ
                                        </TextBlock>
                                    </StackPanel>
                                </Expander.Header>
                                <Grid Margin="0,5,0,0" HorizontalAlignment="Center">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="0" />
                                        <ColumnDefinition Width="0" />
                                        <ColumnDefinition Width="44" />
                                        <ColumnDefinition Width="44" />
                                        <ColumnDefinition Width="44" />
                                        <ColumnDefinition Width="44" />
                                        <ColumnDefinition Width="44" />
                                        <ColumnDefinition Width="44" />
                                        <ColumnDefinition Width="44" />
                                        <ColumnDefinition Width="44" />
                                        <ColumnDefinition Width="44" />
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="35" />
                                        <RowDefinition Height="7" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="50" />
                                        <RowDefinition Height="40" />
                                        <RowDefinition Height="32" />
                                    </Grid.RowDefinitions>

                                    <!--  switch eq  -->
                                    <RepeatButton
                                        x:Name="EQswitch_increment"
                                        Grid.Row="0"
                                        Grid.Column="8"
                                        Grid.ColumnSpan="3"
                                        Margin="2"
                                        Background="{DynamicResource Dark}"
                                        BorderBrush="Transparent"
                                        Click="EQswitch_increment_Click"
                                        FontFamily="roboto black"
                                        FontSize="17"
                                        Foreground="White"
                                        Interval="1"
                                        Style="{StaticResource EQbutton}">
                                        <StackPanel Orientation="Horizontal">

                                            <TextBlock
                                                Margin="0,0,15,0"
                                                VerticalAlignment="Center"
                                                Text="NEXT" />
                                            <Image Width="33" Source="Resources\nextraw.png" />
                                        </StackPanel>
                                    </RepeatButton>
                                    <!--  eq switch value  -->
                                    <Border
                                        Grid.Row="0"
                                        Grid.Column="5"
                                        Grid.ColumnSpan="3"
                                        Margin="2,0,2,0"
                                        BorderBrush="{DynamicResource Dark}"
                                        BorderThickness="3"
                                        CornerRadius="5">
                                        <TextBlock
                                            x:Name="EQswitch_value"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontFamily="roboto black"
                                            FontSize="23"
                                            Foreground="{DynamicResource Foreground}">
                                            0
                                        </TextBlock>
                                    </Border>
                                    <RepeatButton
                                        x:Name="EQswitch_decrement"
                                        Grid.Row="0"
                                        Grid.Column="2"
                                        Grid.ColumnSpan="3"
                                        Margin="2"
                                        Background="{DynamicResource Dark}"
                                        BorderBrush="Transparent"
                                        Click="EQswitch_decrement_Click"
                                        FontFamily="roboto black"
                                        FontSize="17"
                                        Foreground="White"
                                        Interval="1"
                                        Style="{StaticResource EQbutton}">
                                        <StackPanel Orientation="Horizontal">
                                            <Image Width="33" Source="Resources\backraw.png" />
                                            <TextBlock
                                                Margin="15,0,0,0"
                                                VerticalAlignment="Center"
                                                Text="PREV" />
                                        </StackPanel>
                                    </RepeatButton>



                                    <!--  eq slider0  -->
                                    <RepeatButton
                                        x:Name="EQ0_increment"
                                        Grid.Row="2"
                                        Grid.Column="2"
                                        Margin="2"
                                        Background="{DynamicResource Dark}"
                                        BorderBrush="Transparent"
                                        Click="EQ0_increment_Click"
                                        FontFamily="roboto black"
                                        FontSize="32"
                                        Foreground="White"
                                        Interval="1"
                                        Style="{StaticResource EQbutton}">
                                        <TextBlock Margin="0,-2,0,0" Text="+" />
                                    </RepeatButton>
                                    <!--  eq value  -->
                                    <Border
                                        Grid.Row="3"
                                        Grid.Column="2"
                                        Margin="1.5,3,1.5,3"
                                        BorderBrush="{DynamicResource Dark}"
                                        BorderThickness="3"
                                        CornerRadius="5">
                                        <TextBlock
                                            x:Name="EQ0_value"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontFamily="roboto black"
                                            FontSize="17"
                                            Foreground="{DynamicResource Foreground}">
                                            0
                                        </TextBlock>
                                    </Border>
                                    <RepeatButton
                                        x:Name="EQ0_decrement"
                                        Grid.Row="4"
                                        Grid.Column="2"
                                        Margin="2"
                                        Background="{DynamicResource Dark}"
                                        BorderBrush="Transparent"
                                        Click="EQ0_decrement_Click"
                                        FontFamily="roboto black"
                                        FontSize="45"
                                        Foreground="White"
                                        Interval="1"
                                        Style="{StaticResource EQbutton}">
                                        <TextBlock Margin="0,-10,0,0" Text="-" />
                                    </RepeatButton>
                                    <Border
                                        Grid.Row="5"
                                        Grid.Column="2"
                                        Margin="3,5,3,5"
                                        Background="{DynamicResource Dark}"
                                        BorderThickness="0"
                                        CornerRadius="5">
                                        <TextBlock
                                            Margin="0,2,0,0"
                                            HorizontalAlignment="Center"
                                            FontFamily="roboto medium"
                                            FontSize="15">
                                            HP
                                        </TextBlock>
                                    </Border>

                                    <!--  eq slider1  -->
                                    <RepeatButton
                                        x:Name="EQ1_increment"
                                        Grid.Row="2"
                                        Grid.Column="3"
                                        Margin="2"
                                        Background="{DynamicResource Mid}"
                                        BorderBrush="Transparent"
                                        Click="EQ1_increment_Click"
                                        FontFamily="roboto black"
                                        FontSize="32"
                                        Foreground="White"
                                        Interval="1"
                                        Style="{StaticResource EQbutton}">
                                        <TextBlock Margin="0,-2,0,0" Text="+" />
                                    </RepeatButton>
                                    <!--  eq value  -->
                                    <Border
                                        Grid.Row="3"
                                        Grid.Column="3"
                                        Margin="1.5,3,1.5,3"
                                        BorderBrush="{DynamicResource Mid}"
                                        BorderThickness="3"
                                        CornerRadius="5">
                                        <TextBlock
                                            x:Name="EQ1_value"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontFamily="roboto black"
                                            FontSize="17"
                                            Foreground="{DynamicResource Foreground}">
                                            0
                                        </TextBlock>
                                    </Border>
                                    <RepeatButton
                                        x:Name="EQ1_decrement"
                                        Grid.Row="4"
                                        Grid.Column="3"
                                        Margin="2"
                                        Background="{DynamicResource Mid}"
                                        BorderBrush="Transparent"
                                        Click="EQ1_decrement_Click"
                                        FontFamily="roboto black"
                                        FontSize="45"
                                        Foreground="White"
                                        Interval="1"
                                        Style="{StaticResource EQbutton}">
                                        <TextBlock Margin="0,-10,0,0" Text="-" />
                                    </RepeatButton>
                                    <Border
                                        Grid.Row="5"
                                        Grid.Column="3"
                                        Margin="3,5,3,5"
                                        Background="{DynamicResource Mid}"
                                        BorderThickness="0"
                                        CornerRadius="5">
                                        <TextBlock
                                            Margin="0,2,0,0"
                                            HorizontalAlignment="Center"
                                            FontFamily="roboto medium"
                                            FontSize="15">
                                            1
                                        </TextBlock>
                                    </Border>

                                    <!--  eq slider2  -->
                                    <RepeatButton
                                        x:Name="EQ2_increment"
                                        Grid.Row="2"
                                        Grid.Column="4"
                                        Margin="2"
                                        Background="{DynamicResource Light}"
                                        BorderBrush="Transparent"
                                        Click="EQ2_increment_Click"
                                        FontFamily="roboto black"
                                        FontSize="32"
                                        Foreground="White"
                                        Interval="1"
                                        Style="{StaticResource EQbutton}">
                                        <TextBlock Margin="0,-2,0,0" Text="+" />
                                    </RepeatButton>
                                    <!--  eq value  -->
                                    <Border
                                        Grid.Row="3"
                                        Grid.Column="4"
                                        Margin="1.5,3,1.5,3"
                                        BorderBrush="{DynamicResource Light}"
                                        BorderThickness="3"
                                        CornerRadius="5">
                                        <TextBlock
                                            x:Name="EQ2_value"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontFamily="roboto black"
                                            FontSize="17"
                                            Foreground="{DynamicResource Foreground}">
                                            0
                                        </TextBlock>
                                    </Border>
                                    <RepeatButton
                                        x:Name="EQ2_decrement"
                                        Grid.Row="4"
                                        Grid.Column="4"
                                        Margin="2"
                                        Background="{DynamicResource Light}"
                                        BorderBrush="Transparent"
                                        Click="EQ2_decrement_Click"
                                        FontFamily="roboto black"
                                        FontSize="45"
                                        Foreground="White"
                                        Interval="1"
                                        Style="{StaticResource EQbutton}">
                                        <TextBlock Margin="0,-10,0,0" Text="-" />
                                    </RepeatButton>
                                    <Border
                                        Grid.Row="5"
                                        Grid.Column="4"
                                        Margin="3,5,3,5"
                                        Background="{DynamicResource Light}"
                                        BorderThickness="0"
                                        CornerRadius="5">
                                        <TextBlock
                                            Margin="0,2,0,0"
                                            HorizontalAlignment="Center"
                                            FontFamily="roboto medium"
                                            FontSize="15">
                                            2
                                        </TextBlock>
                                    </Border>


                                    <!--  eq slider3  -->
                                    <RepeatButton
                                        x:Name="EQ3_increment"
                                        Grid.Row="2"
                                        Grid.Column="5"
                                        Margin="2"
                                        Background="{DynamicResource Mid}"
                                        BorderBrush="Transparent"
