<UserControl
    x:Class="sierses.Sim.SettingsControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:sierses.Sim"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:styles="clr-namespace:SimHub.Plugins.Styles;assembly=SimHub.Plugins"
    d:DataContext="{d:DesignInstance}"
    mc:Ignorable="d">

    <FrameworkElement.Resources>
        <Style x:Key="IconRectangle" TargetType="{x:Type Rectangle}">
            <Setter Property="UIElement.Opacity" Value="0.9" />
            <Setter Property="Shape.Fill" Value="White" />
            <Setter Property="FrameworkElement.Height" Value="50" />
            <Setter Property="FrameworkElement.Width" Value="50" />
        </Style>
        <Style x:Key="StatusCircle" TargetType="{x:Type Rectangle}">
            <Setter Property="UIElement.Opacity" Value="0.9" />
            <Setter Property="Shape.Fill" Value="White" />
            <Setter Property="FrameworkElement.Height" Value="10" />
            <Setter Property="FrameworkElement.Width" Value="10" />
        </Style>
        <Style x:Key="TabVis" TargetType="{x:Type StackPanel}">
            <Style.Triggers>
                <Trigger Property="UIElement.IsMouseOver" Value="true">
                    <Setter Property="UIElement.Opacity" Value="0.6" />
                </Trigger>
            </Style.Triggers>
        </Style>


        <!--  button style for eq repeat buttons  -->
        <Style x:Key="EQbutton" TargetType="RepeatButton">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="RepeatButton">
                        <Border
                            Background="{TemplateBinding Background}"
                            BorderThickness="0"
                            CornerRadius="5">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Opacity" Value="0.5" />
                            </Trigger>
                            <Trigger Property="IsMouseOver" Value="False">
                                <Setter Property="Opacity" Value="1" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <!--  button style for other buttons curved and opacity  -->
        <Style x:Key="dMASSbutton" TargetType="Button">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border
                            Background="{TemplateBinding Background}"
                            BorderThickness="0"
                            CornerRadius="5">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Opacity" Value="0.5" />
                            </Trigger>
                            <Trigger Property="IsMouseOver" Value="False">
                                <Setter Property="Opacity" Value="1" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>




        <!--  Button corner radius and styling  -->

        <Style TargetType="Button">
            <Style.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="6" />
                    <Setter Property="BorderThickness" Value="0" />
                </Style>
            </Style.Resources>
        </Style>



        <!--  slider style for value label  -->
        <Style x:Key="SliderRepeatButton" TargetType="RepeatButton">
            <Setter Property="SnapsToDevicePixels" Value="true" />
            <Setter Property="OverridesDefaultStyle" Value="true" />
            <Setter Property="IsTabStop" Value="false" />
            <Setter Property="Focusable" Value="false" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="RepeatButton">
                        <Border Background="Transparent" />
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="SliderRepeatButton1" TargetType="RepeatButton">
            <Setter Property="SnapsToDevicePixels" Value="true" />
            <Setter Property="OverridesDefaultStyle" Value="true" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="RepeatButton">
                        <Border
                            Height="3"
                            Background="#4B4495"
                            BorderBrush="#4B4495"
                            BorderThickness="1"
                            SnapsToDevicePixels="True" />
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="SliderThumb" TargetType="Thumb">
            <Setter Property="SnapsToDevicePixels" Value="true" />
            <Setter Property="OverridesDefaultStyle" Value="true" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Thumb">
                        <StackPanel Orientation="Horizontal">
                            <Path
                                x:Name="ThumbdMASS"
                                Margin="-2,0,0,0"
                                Data="M 1 1 L 1 1 L 1 1 Z"
                                Fill="#4B4495"
                                Stroke="#4B4495"
                                StrokeThickness="30" />
                            <!--  text label for thumb  -->
                            <TextBlock
                                x:Name="ThumbdMASSlabel"
                                Width="20"
                                Height="25"
                                Margin="-30,-7.5,0,0"
                                FontFamily="roboto medium"
                                FontSize="13"
                                Foreground="White"
                                Text="{Binding Value, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Slider}}}"
                                TextAlignment="Center" />
                        </StackPanel>
                        <!--  mouse hover color change  -->
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="ThumbdMASS" Property="Stroke" Value="#38345D" />
                                <Setter TargetName="ThumbdMASSlabel" Property="Opacity" Value="0.6" />
                            </Trigger>
                        </ControlTemplate.Triggers>

                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <ControlTemplate x:Key="Slider" TargetType="Slider">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" MinHeight="{TemplateBinding MinHeight}" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>

                <Border BorderBrush="Transparent" BorderThickness="0,0,0,1" />
                <Border
                    x:Name="TrackBackground"
                    Grid.Row="1"
                    Height="3"
                    Margin="0,-10,0,0"
                    VerticalAlignment="Center"
                    Background="White"
                    BorderBrush="White"
                    BorderThickness="1" />
                <Track
                    x:Name="PART_Track"
                    Grid.Row="1"
                    Margin="0,-10,0,0">
                    <Track.DecreaseRepeatButton>
                        <RepeatButton Command="Slider.DecreaseLarge" Style="{StaticResource SliderRepeatButton1}" />
                    </Track.DecreaseRepeatButton>
                    <Track.Thumb>
                        <!--  thumb slider position  -->
                        <Thumb Margin="0,16,0,0" Style="{StaticResource SliderThumb}" />
                    </Track.Thumb>
                    <Track.IncreaseRepeatButton>
                        <RepeatButton Command="Slider.IncreaseLarge" Style="{StaticResource SliderRepeatButton}" />
                    </Track.IncreaseRepeatButton>
                </Track>
            </Grid>
        </ControlTemplate>

        <Style x:Key="Horizontal_Slider" TargetType="Slider">
            <Setter Property="Focusable" Value="False" />
            <Setter Property="SnapsToDevicePixels" Value="true" />
            <Setter Property="OverridesDefaultStyle" Value="true" />
            <Style.Triggers>
                <Trigger Property="Orientation" Value="Horizontal">
                    <Setter Property="MinHeight" Value="21" />
                    <Setter Property="MinWidth" Value="80" />
                    <Setter Property="Template" Value="{StaticResource Slider}" />
                </Trigger>
            </Style.Triggers>
        </Style>
        <!--  slider style for value label end  -->



        <!--  corner radius textboxes
        <Style TargetType="{x:Type TextBox}">
            <Style.Resources>
                <Style TargetType="{x:Type Border}">
                    <Setter Property="CornerRadius" Value="6" />
                    <Setter Property="Height" Value="32" />
                    <Setter Property="BorderThickness" Value="0" />
                    <Setter Property="BorderBrush" Value="Transparent" />
                </Style>
            </Style.Resources>
        </Style>-->

        <!--  corner radius expander start  -->
        <Style x:Key="ExpanderToggleButton" TargetType="{x:Type ToggleButton}">
            <Setter Property="UseLayoutRounding" Value="True" />
            <Setter Property="SnapsToDevicePixels" Value="True" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ToggleButton}">
                        <Grid Width="24" Height="24">
                            <Border
                                x:Name="border"
                                Background="#27234D"
                                CornerRadius="12" />
                            <Path
                                x:Name="arrow"
                                Data="M4.5 9 l4 0 l3.5 4.6667 l3.5 -4.6667 l4 0 l-7.5 10 z"
                                Fill="White" />
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="border" Property="Background" Value="DarkGray" />
                                <Setter TargetName="arrow" Property="Fill" Value="LightGray" />
                            </Trigger>
                            <Trigger Property="IsChecked" Value="True">
                                <Setter TargetName="border" Property="Background" Value="White" />
                                <Setter TargetName="arrow" Property="Fill" Value="#27234D" />
                                <Setter TargetName="arrow" Property="Data" Value="M4.5 15 l4 0 l3.5 -4.6667 l3.5 4.6667 l4 0 l-7.5 -10 z" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style TargetType="{x:Type Expander}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Expander}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition x:Name="ContentRow" Height="0" />
                            </Grid.RowDefinitions>
                            <Border
                                x:Name="Border"
                                Grid.Row="0"
                                Background="#4B4495"
                                BorderThickness="0"
                                CornerRadius="2,0,10,0">
                                <!--  color arround arrow  -->
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="31" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <ToggleButton
                                        Grid.Column="0"
                                        IsChecked="{Binding IsExpanded, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                        OverridesDefaultStyle="True"
                                        Style="{StaticResource ExpanderToggleButton}" />
                                    <!--  This is the border that puts the styling around the Expander Header  -->
                                    <!--  Add more detail here for a fancier header.  -->
                                    <Border
                                        Grid.Column="1"
                                        Background="#4B4495"
                                        CornerRadius="0,0,10,0">
                                        <ContentPresenter
                                            Height="30"
                                            Margin="10,5,0,0"
                                            ContentSource="Header"
                                            RecognizesAccessKey="True"
                                            TextBlock.FontFamily="roboto"
                                            TextBlock.FontSize="20"
                                            TextBlock.Foreground="White" />
                                    </Border>

                                </Grid>
                            </Border>
                            <Border
                                x:Name="Content"
                                Grid.Row="1"
                                BorderThickness="0"
                                CornerRadius="0,0,2,2">
                                <ContentPresenter Margin="4" />
                            </Border>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsExpanded" Value="True">
                                <Setter TargetName="ContentRow" Property="Height" Value="{Binding DesiredSize, ElementName=Content}" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <!--  corner radius expander end  -->


    </FrameworkElement.Resources>
    <Grid>
        <TabControl>
            <!--  custom tab control for no vertical wrap  -->
            <TabControl.Template>
                <ControlTemplate TargetType="{x:Type TabControl}">
                    <Grid
                        ClipToBounds="true"
                        KeyboardNavigation.TabNavigation="Local"
                        SnapsToDevicePixels="true">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition x:Name="ColumnDefinition0" />
                            <ColumnDefinition x:Name="ColumnDefinition1" Width="0" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition x:Name="RowDefinition0" Height="Auto" />
                            <RowDefinition x:Name="RowDefinition1" Height="*" />
                        </Grid.RowDefinitions>
                        <StackPanel
                            x:Name="HeaderPanel"
                            Grid.Row="0"
                            Grid.Column="0"
                            Margin="2,2,2,0"
                            Panel.ZIndex="1"
                            IsItemsHost="true"
                            KeyboardNavigation.TabIndex="1"
                            Orientation="Horizontal" />
                        <Border
                            x:Name="ContentPanel"
                            Grid.Row="1"
                            Grid.Column="0"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            KeyboardNavigation.DirectionalNavigation="Contained"
                            KeyboardNavigation.TabIndex="2"
                            KeyboardNavigation.TabNavigation="Local">
                            <ContentPresenter
                                x:Name="PART_SelectedContentHost"
                                Margin="{TemplateBinding Padding}"
                                ContentSource="SelectedContent"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="TabStripPlacement" Value="Bottom">
                            <Setter TargetName="HeaderPanel" Property="Grid.Row" Value="1" />
                            <Setter TargetName="ContentPanel" Property="Grid.Row" Value="0" />
                            <Setter TargetName="RowDefinition0" Property="Height" Value="*" />
                            <Setter TargetName="RowDefinition1" Property="Height" Value="Auto" />
                            <Setter TargetName="HeaderPanel" Property="Margin" Value="2,0,2,2" />
                        </Trigger>
                        <Trigger Property="TabStripPlacement" Value="Left">
                            <Setter TargetName="HeaderPanel" Property="Grid.Row" Value="0" />
                            <Setter TargetName="ContentPanel" Property="Grid.Row" Value="0" />
                            <Setter TargetName="HeaderPanel" Property="Grid.Column" Value="0" />
                            <Setter TargetName="ContentPanel" Property="Grid.Column" Value="1" />
                            <Setter TargetName="ColumnDefinition0" Property="Width" Value="Auto" />
                            <Setter TargetName="ColumnDefinition1" Property="Width" Value="*" />
                            <Setter TargetName="RowDefinition0" Property="Height" Value="*" />
                            <Setter TargetName="RowDefinition1" Property="Height" Value="0" />
                            <Setter TargetName="HeaderPanel" Property="Margin" Value="2,2,0,2" />
                        </Trigger>
                        <Trigger Property="TabStripPlacement" Value="Right">
                            <Setter TargetName="HeaderPanel" Property="Grid.Row" Value="0" />
                            <Setter TargetName="ContentPanel" Property="Grid.Row" Value="0" />
                            <Setter TargetName="HeaderPanel" Property="Grid.Column" Value="1" />
                            <Setter TargetName="ContentPanel" Property="Grid.Column" Value="0" />
                            <Setter TargetName="ColumnDefinition0" Property="Width" Value="*" />
                            <Setter TargetName="ColumnDefinition1" Property="Width" Value="Auto" />
                            <Setter TargetName="RowDefinition0" Property="Height" Value="*" />
                            <Setter TargetName="RowDefinition1" Property="Height" Value="0" />
                            <Setter TargetName="HeaderPanel" Property="Margin" Value="0,2,2,2" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </TabControl.Template>
            <!--  custom tab control for no vertical wrap end  -->
            <TabItem
                Width="160"
                Padding="0,10,0,10"
                HorizontalAlignment="Left"
                VerticalAlignment="Center">
                <HeaderedContentControl.Header>
                    <StackPanel
                        Margin="0,0"
                        Orientation="Horizontal"
                        Style="{StaticResource TabVis}">
                        <Rectangle Style="{StaticResource IconRectangle}">
                            <UIElement.OpacityMask>
                                <ImageBrush ImageSource="{Binding RPMIcon}" RenderOptions.BitmapScalingMode="HighQuality" />
                            </UIElement.OpacityMask>
                        </Rectangle>
                        <TextBlock
                            Margin="5,0,0,0"
                            VerticalAlignment="Center"
                            FontFamily="roboto medium"
                            FontSize="26"
                            Opacity="0.9"
                            Text="Engine" />
                    </StackPanel>
                </HeaderedContentControl.Header>

                <Grid>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="30" />
                    </Grid.RowDefinitions>

                    <ScrollViewer VerticalScrollBarVisibility="Hidden">
                        <StackPanel
                            Margin="0,10,0,0"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Top"
                            Orientation="vertical">
                            <!--  EXPANDER Engine RPM properties  -->
                            <Expander
                                BorderThickness="2"
                                FontFamily="roboto"
                                FontSize="15"
                                IsExpanded="True">
                                <Expander.Header>
                                    <StackPanel Margin="-4,-2,0,0" Orientation="Horizontal">
                                        <Image
                                            Width="29"
                                            Margin="0,-3,10,0"
                                            Source="C:\Users\demas\source\repos\SimHap\Resources\engine50x50.png" />
                                        <TextBlock
                                            Margin="-7,-5,0,0"
                                            VerticalAlignment="Center"
                                            FontFamily="roboto"
                                            FontSize="20"
                                            Foreground="White">
                                            Engine Properties
                                        </TextBlock>
                                    </StackPanel>
                                </Expander.Header>
                                <StackPanel>

                                    <!--  CarName  -->
                                    <StackPanel
                                        Margin="0,15,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Orientation="Horizontal">
                                        <TextBlock VerticalAlignment="Center" TextAlignment="Right"><Run Text="Name" /></TextBlock>
                                    </StackPanel>
                                    <StackPanel Margin="0,2,0,0">
                                        <TextBox
                                            x:Name="CarName"
                                            Width="400"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            Background="Transparent"
                                            BorderBrush="#4B4495"
                                            BorderThickness="0,0,0,2"
                                            FontFamily="roboto"
                                            FontSize="16"
                                            Text="{Binding S.CarName}" />
                                    </StackPanel>

                                    <!--  ID  -->
                                    <StackPanel
                                        Margin="0,10,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Orientation="Horizontal">
                                        <TextBlock VerticalAlignment="Center" TextAlignment="Right"><Run Text="ID" /></TextBlock>
                                    </StackPanel>
                                    <StackPanel Margin="0,2,0,0">
                                        <TextBox
                                            x:Name="CarID"
                                            Width="400"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            Background="Transparent"
                                            BorderBrush="#4B4495"
                                            BorderThickness="0,0,0,2"
                                            FontFamily="roboto"
                                            FontSize="16"
                                            Text="{Binding S.Id}" />
                                    </StackPanel>

                                    <!--  Category  -->
                                    <StackPanel
                                        Margin="0,10,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Orientation="Horizontal">
                                        <TextBlock VerticalAlignment="Center" TextAlignment="Right"><Run Text="Category" /></TextBlock>
                                    </StackPanel>
                                    <StackPanel Margin="0,2,0,0">
                                        <TextBox
                                            x:Name="CarClass"
                                            Width="400"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            Background="Transparent"
                                            BorderBrush="#4B4495"
                                            BorderThickness="0,0,0,2"
                                            FontFamily="roboto"
                                            FontSize="16"
                                            Text="{Binding S.Category}" />
                                    </StackPanel>


                                    <!--  Idle/Redline/MaxRPM headers  -->
                                    <StackPanel
                                        Margin="0,10,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Orientation="Horizontal">
                                        <TextBlock
                                            MinWidth="80"
                                            Margin="0,0,80,0"
                                            TextAlignment="Center"><Run Text="Idle" /></TextBlock>
                                        <TextBlock
                                            MinWidth="80"
                                            Margin="0,0,80,0"
                                            TextAlignment="Center"><Run Text="Redline" /></TextBlock>
                                        <TextBlock
                                            MinWidth="80"
                                            Margin="0,0,0,0"
                                            TextAlignment="Center"><Run Text="Max" /></TextBlock>
                                    </StackPanel>
                                    <!--  Idle/Redline/MaxRPM textboxes  -->
                                    <StackPanel
                                        Margin="0,2,0,0"
                                        HorizontalAlignment="Center"
                                        Orientation="Horizontal">
                                        <TextBox
                                            x:Name="IdleRPM"
                                            MinWidth="80"
                                            Margin="0,0,80,0"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            Background="Transparent"
                                            BorderBrush="#4B4495"
                                            BorderThickness="0,0,0,2"
                                            FontFamily="roboto"
                                            FontSize="16"
                                            Text="{Binding S.IdleRPM}" />
                                        <TextBox
                                            x:Name="Redline"
                                            MinWidth="80"
                                            Margin="0,0,80,0"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            Background="Transparent"
                                            BorderBrush="#4B4495"
                                            BorderThickness="0,0,0,2"
                                            FontFamily="roboto"
                                            FontSize="16"
                                            Text="{Binding S.Redline}" />
                                        <TextBox
                                            x:Name="MaxRPM"
                                            MinWidth="80"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            Background="Transparent"
                                            BorderBrush="#4B4495"
                                            BorderThickness="0,0,0,2"
                                            FontFamily="roboto"
                                            FontSize="16"
                                            Text="{Binding S.MaxRPM}" />
                                    </StackPanel>

                                    <!--  Engine config/cylinders/drivetrain headers  -->
                                    <StackPanel
                                        Margin="0,10,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Orientation="Horizontal">
                                        <TextBlock
                                            MinWidth="80"
                                            Margin="0,0,80,0"
                                            VerticalAlignment="Center"
                                            TextAlignment="Center"><Run Text="Config" /></TextBlock>
                                        <TextBlock
                                            MinWidth="80"
                                            Margin="0,0,80,0"
                                            VerticalAlignment="Center"
                                            TextAlignment="Center"><Run Text="Cylinders" /></TextBlock>
                                        <TextBlock
                                            MinWidth="80"
                                            VerticalAlignment="Center"
                                            TextAlignment="Center"><Run Text="Drivetrain" /></TextBlock>
                                    </StackPanel>

                                    <!--  Engine config/cylinders/drivetrain comboboxes  -->
                                    <StackPanel
                                        Margin="0,2,0,0"
                                        HorizontalAlignment="Center"
                                        Orientation="Horizontal">
                                        <!--  engine config  -->
                                        <ComboBox
                                            Name="EngineConfig"
                                            Width="80"
                                            Height="32"
                                            Margin="0,0,80,0"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            FontFamily="roboto"
                                            FontSize="16"
                                            Text="{Binding S.EngineConfiguration}">
                                            <ComboBoxItem Content="V" />
                                            <ComboBoxItem Content="I" />
                                            <ComboBoxItem Content="F" />
                                            <ComboBoxItem Content="E" />
                                            <ComboBoxItem Content="W" />
                                            <ComboBoxItem Content="R" />
                                        </ComboBox>
                                        <!--  Cylinders  -->
                                        <ComboBox
                                            Name="EngineCylinders"
                                            Width="80"
                                            Margin="0,0,80,0"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            FontFamily="roboto"
                                            FontSize="16"
                                            Text="{Binding S.EngineCylinders}">
                                            <ComboBoxItem Content="1" />
                                            <ComboBoxItem Content="2" />
                                            <ComboBoxItem Content="3" />
                                            <ComboBoxItem Content="4" />
                                            <ComboBoxItem Content="5" />
                                            <ComboBoxItem Content="6" />
                                            <ComboBoxItem Content="8" />
                                            <ComboBoxItem Content="10" />
                                            <ComboBoxItem Content="12" />
                                            <ComboBoxItem Content="16" />
                                        </ComboBox>
                                        <ComboBox
                                            Name="Drivetrain"
                                            Width="80"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            FontFamily="roboto"
                                            FontSize="16"
                                            Text="{Binding S.PoweredWheels}">
                                            <ComboBoxItem Content="F" />
                                            <ComboBoxItem Content="R" />
                                            <ComboBoxItem Content="A" />
                                        </ComboBox>
                                    </StackPanel>

                                    <!--  Engine location/hp/nm headers  -->
                                    <StackPanel
                                        Margin="0,10,0,0"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Orientation="Horizontal">

                                        <TextBlock
                                            MinWidth="80"
                                            Margin="0,0,80,0"
                                            VerticalAlignment="Center"
                                            TextAlignment="Center"><Run Text="Location" /></TextBlock>
                                        <TextBlock
                                            MinWidth="80"
                                            Margin="0,0,80,0"
                                            VerticalAlignment="Center"
                                            TextAlignment="Center"><Run Text="HP" /></TextBlock>
                                        <TextBlock
                                            MinWidth="80"
                                            VerticalAlignment="Center"
                                            TextAlignment="Center"><Run Text="NM" /></TextBlock>
                                    </StackPanel>
                                    <!--  Engine location/hp/nm comboboxes and textboxes  -->
                                    <StackPanel
                                        Margin="0,2,0,0"
                                        HorizontalAlignment="Center"
                                        Orientation="Horizontal">
                                        <ComboBox
                                            Name="Englocation"
                                            Width="80"
                                            Margin="0,0,80,0"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            FontFamily="roboto"
                                            FontSize="16"
                                            Text="{Binding S.EngineLocation}">
