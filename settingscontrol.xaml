                                            </TextBlock>
                                            <TextBlock
                                                Name="LabelPitchMult"
                                                Width="150"
                                                VerticalAlignment="Center"
                                                FontFamily="roboto medium"
                                                Foreground="{DynamicResource Foreground}"
                                                TextAlignment="Center">
                                                Pitch Multi
                                            </TextBlock>
                                            <TextBlock
                                                Name="LabelPitchGamma"
                                                Width="150"
                                                VerticalAlignment="Center"
                                                FontFamily="roboto medium"
                                                Foreground="{DynamicResource Foreground}"
                                                TextAlignment="Center">
