    <SolidColorBrush x:Key="Light" Color="#76a547" />
    <SolidColorBrush x:Key="LightMouse" Color="#548e19" />

    <SolidColorBrush x:Key="Mid" Color="#548e19" />
    <SolidColorBrush x:Key="MidMouse" Color="#437214" />

    <SolidColorBrush x:Key="Dark" Color="#3b6312" />
