    <SolidColorBrush x:Key="Light" Color="#a13951" />
    <SolidColorBrush x:Key="LightMouse" Color="#890725" />

    <SolidColorBrush x:Key="Mid" Color="#890725" />
    <SolidColorBrush x:Key="MidMouse" Color="#6e061e" />

    <SolidColorBrush x:Key="Dark" Color="#60051a" />
